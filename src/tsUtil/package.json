{
  "name": "wings-ts-util",
  "version": "0.0.6",
  "description": "Utility class for Wings (a codegen module).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "pretest": "tsc scripts/testPrep.ts --outDir ./ && node ./testPrep.js && rm ./*.js",
    "posttest": "tsc scripts/testCleanup.ts --outDir ./ && node ./testCleanup.js && rm ./*.js",
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "test": "npm run pretest && mocha && npm run posttest",
    "cover": "npm run pretest && nyc mocha && npm run posttest",
    "clean": "rm -rf dist .nyc_output",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binhonglee/wings.git",
    "directory": "src/tsUtil"
  },
  "author": "BinHong Lee",
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "1.4.7",
    "@types/mocha": "7.0.2",
    "@types/node": "^14.0.5",
    "mocha": "7.2.0",
    "nyc": "15.0.1",
    "ts-node": "8.10.2",
    "typescript": "3.9.3"
  },
  "nyc": {
    "instrument": true,
    "sourceMaps": false,
    "all": true,
    "check-coverage": false,
    "per-file": true,
    "clean": true,
    "temp-dir": "./.nyc_output",
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/scripts/*.ts",
      "**/*.{spec,test}.{js,ts}",
      "*.js"
    ],
    "reporter": [
      "lcov",
      "cobertura",
      "text",
      "text-summary"
    ],
    "report-dir": "../../report",
    "extension": [
      ".ts"
    ]
  }
}
