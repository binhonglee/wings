{
  "name": "wings-ts-util",
  "version": "0.0.6",
  "description": "Utility class for Wings (a codegen module).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "test": "mocha",
    "cover": "nyc mocha",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/binhonglee/wings.git",
    "directory": "src/tsUtil"
  },
  "author": "BinHong Lee",
  "license": "MIT",
  "devDependencies": {
    "@types/assert": "1.4.6",
    "@types/mocha": "7.0.2",
    "mocha": "7.1.1",
    "nyc": "15.0.0",
    "ts-node": "8.7.0",
    "typescript": "3.8.3"
  },
  "nyc": {
    "instrument": true,
    "sourceMaps": false,
    "all": true,
    "check-coverage": false,
    "per-file": true,
    "clean": true,
    "temp-dir": "./.nyc_output",
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "**/*.{spec,test}.{js,ts}",
      "*.js"
    ],
    "reporter": [
      "lcov",
      "cobertura",
      "text",
      "text-summary"
    ],
    "report-dir": "../../report",
    "extension": [
      ".ts"
    ]
  }
}
