{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Wings",
  "scopeName": "source.wings",
  "patterns": [{
    "include": "#keywords"
  }, {
    "include": "#filepath"
  }, {
    "include": "#code-comment"
  }, {
    "include": "#wings-comment"
  }, {
    "include": "#wings-types"
  }, {
    "include": "#go-func"
  }, {
    "include": "#js-func"
  }, {
    "include": "#kt-func"
  }, {
    "include": "#nim-func"
  }, {
    "include": "#py-func"
  }, {
    "include": "#ts-func"
  }],
  "repository": {
    "keywords": {
      "begin": "^( |\t)*([a-z]+-(import|implement))\\b",
      "end": "\n",
      "name": "storage.modifier",
      "patterns": [{
        "name": "support.class",
        "begin": "( |\t)+[a-zA-Z {}]+",
        "end": "(?=\n)",
        "patterns": [{
          "name": "storage.modifier",
          "begin": ":",
          "end": "(?=\n)",
          "patterns": [{
            "name": "string.unquoted",
            "match": "[a-zA-Z\\-_\\.]+[\/][a-zA-Z\\-_\\.\/]+"
          }, {
            "name": "entity.name.tag",
            "match": ".*"
          }]
        }]
      }, {
        "name": "support.class",
        "match": ".*"
      }]
    },
    "filepath": {
      "begin": "^( |\t)*([a-z]+-(filepath)|import)\\b",
      "end": "\n",
      "name": "storage.modifier",
      "patterns": [{
        "name": "string.unquoted",
        "match": ".*"
      }]
    },
    "code-comment": {
      "name": "comment.line.double-slash",
      "match": "//( |\t)*(.*)$"
    },
    "wings-comment": {
      "name": "comment.line.number-sign",
      "match": "^#( |\t)+(.*)$"
    },
    "core-types": {
      "name": "entity.name.tag",
      "match": "([\\[\\]])*(int|flt|dbl|str|char|bool|date)"
    },
    "map": {
      "name": "support.class",
      "begin": "(Map|Set)<",
      "end": ">( |\n|\t)",
      "captures": {
        "1": {
          "patterns": [{
            "match": "[,]"
          }]
        },
        "2": {
          "patterns": [{
            "begin": "Map<",
            "end": ">"
          }]
        }
      },
      "patterns": [{
        "include": "#core-types"
      }, {
        "include": "$self"
      }]
    },
    "wings-types": {
      "begin": "^( |\t)*(interface|struct|enum)",
      "end": "}",
      "name": "storage.modifier",
      "patterns": [{
        "begin": " [A-Z]+[a-zA-Z]* ",
        "end": "(?=})",
        "name": "entity.name.class",
        "patterns": [{
          "begin": "{",
          "end": "(?=})",
          "name": "storage.modifier",
          "patterns": [{
            "begin": "\n",
            "end": "(?=})",
            "name": "invalid",
            "patterns": [{
              "include": "#code-comment"
            }, {
              "name": "string.quoted.double",
              "begin": "\"",
              "end": "\"",
              "patterns": [{
                "name": "constant.character.escape",
                "match": "\\\\."
              }]
            }, {
              "begin": "'",
              "beginCaptures": {
                "0": {
                  "patterns": [{
                    "match": "'"
                  }]
                }
              },
              "end": "'",
              "endCaptures": {
                "0": {
                  "patterns": [{
                    "match": "'"
                  }]
                }
              },
              "name": "constant.character",
              "patterns": [{
                "match": "\\\\.",
                "name": "constant.character.escape.java"
              }]
            }, {
              "name": "entity.name.tag",
              "match": "( |\t)*([\\[\\]])*(int|flt|dbl|str|char|bool|date)( |\t|\n)"
            }, {
              "name": "support.class",
              "match": "( |\t)*([\\[\\]])*([A-Z]+[a-zA-Z0-9]+)( |\t|\n)"
            }, {
              "include": "#map"
            }, {
              "name": "constant.numeric",
              "match": "( |\t)*([\\-][0-9]+)(\n)"
            }, {
              "name": "constant.language",
              "match": "( |\t)*(true|false|null)(\n)"
            }, {
              "name": "support.constant",
              "match": "( |\t)*(([a-zA-Z0-9]|[_]|[.])+)(\n)"
            }, {
              "name": "variable.other",
              "match": "( |\t)*([a-zA-Z0-9]|[_])+( |\t)"
            }]
          }]
        }]
      }]
    },
    "go-func": {
      "begin": "go\\-func",
      "end": "^\\)\n",
      "name": "support.function",
      "patterns": [{
        "begin": "\\(",
        "end": "(?=^\\)\n)",
        "contentName": "source.go",
        "patterns": [{
          "include": "source.go"
        }]
      }]
    },
    "js-func": {
      "begin": "^js\\-func",
      "end": "^\\)\n",
      "name": "support.function",
      "patterns": [{
        "begin": "\\(",
        "end": "(?=^\\)\n)",
        "name": "source.js",
        "patterns": [{
          "include": "source.js"
        }]
      }]
    },
    "kt-func": {
      "begin": "^kt\\-func",
      "end": "^\\)\n",
      "name": "support.function",
      "patterns": [{
        "begin": "\\(",
        "end": "(?=^\\)\n)",
        "name": "source.kotlin",
        "patterns": [{
          "include": "source.kotlin"
        }]
      }]
    },
    "nim-func": {
      "begin": "^nim\\-func",
      "end": "^\\)\n",
      "name": "support.function",
      "patterns": [{
        "begin": "\\(",
        "end": "(?=^\\)\n)",
        "name": "source.nim",
        "patterns": [{
          "include": "source.nim"
        }]
      }]
    },
    "py-func": {
      "begin": "^py\\-func",
      "end": "^\\)\n",
      "name": "support.function",
      "patterns": [{
        "begin": "\\(",
        "end": "(?=^\\)\n)",
        "name": "source.python",
        "patterns": [{
          "include": "source.python"
        }]
      }]
    },
    "ts-func": {
      "begin": "^ts\\-func",
      "end": "^\\)\n",
      "name": "support.function",
      "patterns": [{
        "begin": "\\(",
        "end": "(?=^\\)\n)",
        "name": "source.ts",
        "patterns": [{
          "include": "source.ts"
        }]
      }]
    }
  }
}
