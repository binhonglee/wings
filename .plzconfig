[Please]
version = 16.1.0

[Build]
PassEnv = PATH
PassEnv = USER
PassEnv = OSTYPE

[Alias "genFile"]
cmd = run //src/main:wings

[Alias "docs"]
cmd = run //src/main/scripts:docs -- serve

[Alias "genDocs"]
cmd = run //src/main/scripts:docs -- build

[Alias "release"]
cmd = run //src/main/scripts:release

[Alias "lang"]
cmd = run //src/main/staticlang:static
